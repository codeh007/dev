{
  "image": "gitgit188/gomtm:latest",
  "hostRequirements": {
    "cpus": 4
  },
  "waitFor": "onCreateCommand",
  "updateContentCommand": "",
  "postCreateCommand": "sudo npm i -g gomtm-cli",
  "postAttachCommand": {
    "server": ""
  },
  "privileged": true,
  "customizations": {
    "codespaces": {
      "openFiles": [
        "pages/index.js"
      ]
    }
  },
  "portsAttributes": {
    "8080": {
      "label": "Application",
      "onAutoForward": "openPreview"
    }
  },
  
  "forwardPorts": [8080],
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {
      "version": "latest"
    }
  },
  "remoteUser": "user"
}
